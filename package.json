  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@vercel/node": "^1.11.1",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "typescript": "^4.3.4"
  }
}
